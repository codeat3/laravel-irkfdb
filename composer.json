{
    "name": "codeat3/laravel-irkfdb",
    "description": "",
    "keywords": [
        "codeat3",
        "laravel-irkfdb"
    ],
    "homepage": "https://github.com/codeat3/laravel-irkfdb",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Swapnil Sarwe",
            "email": "swapnilsarwe@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2",
        "illuminate/support": "^7.0",
        "kitetail/zttp": "^0.6.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "orchestra/testbench": "^5.0",
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "psr-4": {
            "Codeat3\\LaravelIrkfdb\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Codeat3\\LaravelIrkfdb\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": [
            "vendor/bin/php-cs-fixer fix"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Codeat3\\LaravelIrkfdb\\LaravelIrkfdbServiceProvider"
            ],
            "aliases": {
                "LaravelIrkfdb": "Codeat3\\LaravelIrkfdb\\LaravelIrkfdbFacade"
            }
        }
    }
}
